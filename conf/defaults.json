{
  "statfulAwsCollector": {
    "credentials": {
      "accessKeyId": "foo",
      "secretAccessKey": "bar"
    },
    "period": 60,
    "statistics": ["SampleCount", "Average", "Sum", "Minimum", "Maximum"],
    "metricsList": {
      "type": "black",
      "metrics": {}
    },
    "signals": [
      "SIGTERM",
      "SIGINT",
      "SIGABRT",
      "SIGUSR2"
    ]
  },
  "bunyan": {
    "name": "statfulAwsCollector",
    "streams": []
  },
  "statfulClient": {
    "transport": "api",
    "api": {
      "token": "@YOUR_API_TOKEN"
    },
    "app": "statful-aws-collector",
    "flushInterval": 30000
  }
}
